{
    "spellright.language": [
        "en"
    ],
    "files.exclude": {
        "**/.vs": true,
        "**/bin": true,
        "**/obj": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/**.egg-info": true,
        "**/__pycache__": true,
        "**/**.dist-info": true,
        "dist": true,
        "dc/build": true,
        ".venv/include": true,
        ".venv/pyvenv.cfg": true,
    },
    "python.formatting.autopep8Args": [
        "--max-line-length=200"
    ],
    "isort.args": [
        "-l",
        "120",
        "--lbt",
        "1"
    ],
    "restructuredtext.languageServer.disabled": true,
    "restructuredtext.confPath": "",
    "restructuredtext.linter.disabled": true
}